/* pages/tripList/tripList.wxss */
.trip_list{
  width: 100%;
  background: ghostwhite;
  display: flex;
  flex-direction: column;

}

.trip_list_cell{
  background: white;
  box-shadow: 2rpx 2rpx 10rpx 3rpx #aaa; /*for Android*/
  -webkit-box-shadow: 2rpx 2rpx 10rpx 3rpx #aaa; /*for IOS*/
  border-radius: 10rpx;
  margin: 10rpx 20rpx;
  padding: 10rpx 10rpx;
 display: flex;
  flex-direction: column;
}

.bottomView{
  display: flex;
  justify-content: space-between;
  margin-top: 10rpx;
}