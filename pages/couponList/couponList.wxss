/* pages/couponList/couponList.wxss */

.backgroundView{
  width: 100%;
  background: ghostwhite;
  display: flex;
  flex-direction: column;

}

.coupon_list_cell{
  background: white;
  box-shadow: 2rpx 2rpx 10rpx 3rpx #aaa; /*for Android*/
  -webkit-box-shadow: 2rpx 2rpx 10rpx 3rpx #aaa; /*for IOS*/
  border-radius: 10rpx;
  margin: 10rpx 20rpx;
  display: flex;
  height: 200rpx;
  position: relative;
}

.moneyView{
  background-color: green;
  width: 200rpx;
  color: white;
  align-items: center;
  justify-content: center;
  display: flex;
  font-size: 36rpx;
}
.detailView{
  position: absolute;
  right: 0;
  left: 200rpx;
  bottom: 0;
  top: 0;
}

.titleName{
  display: flex;
  width: 100%;
  height: 50%;
  align-items: center;
  justify-content: center;
  font-size: 40rpx;
}
.expirationDate{
  position: absolute;
  bottom: 10rpx;
  left: 10rpx;
  color: gray;
}